# Name suffix
exe_suffix = win64
BLD_DIR = saldl-${exe_suffix}

# mingw-w64 binaries
CC = x86_64-w64-mingw32-gcc
STRIP = x86_64-w64-mingw32-strip

# curl dirs
MINGW_LIB_PATH = /usr/x86_64-w64-mingw32/lib
MINGW_INCLUDE_PATH = /usr/x86_64-w64-mingw32/include

# libevent dirs
LIBEVENT_LIB_PATH = ./libevent-2.1.5-beta-130-g0ec3b6d-win64/lib
LIBEVENT_INCLUDE_PATH = ./libevent-2.1.5-beta-130-g0ec3b6d-win64/include

include Makefile.mingw.include
